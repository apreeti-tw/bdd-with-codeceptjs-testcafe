{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {"code": 120, "ignoreStrings": true}],
        "semi": [2, "never"]
    },
    "overrides": [{
        "files": ["*.js"],
        "rules": {
            "require-jsdoc": "off"
        }
    }, {
        "files": ["*.js"],
        "rules": {
            "new-cap": "off"
        }
    }]
}
